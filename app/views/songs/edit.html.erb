<h1>Songs#edit</h1>
<p>Find me in app/views/songs/edit.html.erb</p>
<%= form_for(@song) do |f| %>
    <label>Song name:</label><br>
    <%= f.text_field :name %><br>

    <label>Song artist_id:</label><br>
    <%= f.text_field :artist_id %><br>

    <label>Song genre_id:</label><br>
    <%= f.text_field :genre_id %><br>

    <%= f.submit %>
  <% end %>